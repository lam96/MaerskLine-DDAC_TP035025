@model IEnumerable<MaerskLine.Models.Schedule>
@{
    ViewBag.Title = "Index";
}

<h2>Index/View Schedule</h2>

<p>
    @Html.ActionLink("Create New", "New")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(m => m.Origin)
        </th>
        <th>
            @Html.DisplayNameFor(m => m.Destination)
        </th>
        <th>
            @Html.DisplayNameFor(m => m.DepartTime)
        </th>
        <th>
            @Html.DisplayNameFor(m => m.ArriveTime)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Origin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Destination)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DepartTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ArriveTime)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.ScheduleId }) |
                @Html.ActionLink("Details", "ScheduleDetails", new { id = item.ScheduleId }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.ScheduleId })
            </td>
        </tr>
    }

</table>

