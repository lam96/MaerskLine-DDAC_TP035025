
@{
    ViewBag.Title = "SelectSchedule";
}

<h2>SelectSchedule</h2>

<table class="table">
<tr>
    <th>
        Origin
    </th>
    <th>
        Destination
    </th>
    <th>
        DepartureTime
    </th>
    <th>
        ArrivalTime
    </th>
    <th></th>
</tr>

@foreach (var item in Model)
{
    <tr>
        <td>
            @item.Origin
        </td>
        <td>
            @item.Destination
        </td>
        <td>
            @item.DepartTime
        </td>
        <td>
            @item.ArriveTime
        </td>
        <td>
            @Html.ActionLink("Select", "ChooseShip", "Booking", new { ScheduleId = item.ScheduleId }, new { @class = "btn btn-success btn-xs" })
        </td>
    </tr>
}

</table>

